@model SmartStore.Web.Models.Catalog.VitaeMenuBarModel
<!-- Header -->
<header id="site_header" class="header">
    <div class="header-content clearfix">

        <!-- Text Logo -->
        <div class="text-logo">
            <a href="index.html">
                <div class="logo-symbol">V</div>
                <div class="logo-text">Curriculum <span>Vitae</span></div>
            </a>
        </div>
        <!-- /Text Logo -->
        <!-- Navigation -->
        <div class="site-nav mobile-menu-hide">
            <ul class="leven-classic-menu site-main-menu">
                <li class="menu-item">
                    <a href="@Url.Action("Index", "Vitae", new { customerId = @Model.CustomerID })">About Me</a>
                </li>

                <li class="menu-item">
                    <a href="@Url.Action("Resume", "Vitae", new { customerId = @Model.CustomerID })">Resume</a>
                </li>

                <li class="menu-item">
                    <a href="resume.html">Portfolio</a>
                </li>

                <li class="menu-item">
                    <a href="contact.html">Contact</a>
                </li>
            </ul>
        </div>

        <!-- Mobile Menu Toggle -->
        <a class="menu-toggle mobile-visible">
            <i class="fa fa-bars"></i>
        </a>
        <!-- Mobile Menu Toggle -->
    </div>
</header>
<!-- /Header -->